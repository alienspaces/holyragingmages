{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "http://holyragingmages.com/schema/templates/data.schema.json",
    "title": "Templates Data",
    "description": "Payload data for templates",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "id"
    ],
    "properties": {
        "id": {
            "description": "Resource ID",
            "type": "string",
            "format": "uuid",
            "example": "a87feca8-d6f0-4794-98c7-037b30219520",
            "readOnly": true
        },
        "created_at": {
            "description": "",
            "type": "string",
            "format": "date-time",
            "readOnly": true
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
        }
    }
}