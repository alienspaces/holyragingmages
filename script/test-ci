#!/usr/bin/env bash

cp .env.test .env
status=0

# environment
source ${BASH_SOURCE%/*}/environment || status=$?
if [ $status -ne 0 ]; then
    echo "Failed including environment" >&2
    exit $status
fi

# test
source ${BASH_SOURCE%/*}/test || status=$?
if [ $status -ne 0 ]; then
    echo "Failed test" >&2
    exit $status
fi
