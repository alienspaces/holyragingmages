#!/usr/bin/env bash

# environment
cp .env.test .env
status=0

source ${BASH_SOURCE%/*}/environment || status=$?
if [ $status -ne 0 ]; then
    echo "Failed including environment" >&2
    exit $status
fi

source ${BASH_SOURCE%/*}/test || status=$?
if [ $status -ne 0 ]; then
    echo "Failed test" >&2
    exit $status
fi
